            },
            "y2": {"field": "Value"},
            "color": {"value": "#2e86c1"}
          }
        },
        {
          "data": {"values": [{}]},
          "encoding": {"y": {"datum": 49.8}},
          "layer": [
            {"mark": "rule"},
            {
              "mark": {
                "type": "text",
                "align": "right",
                "baseline": "bottom",
                "dx": -2,
