{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "hconcat": [
    {
      "width": "container",
      "height": 400,
      "title": "Paramedic Witnessed Cardiac Arrest Survival Rates (2021)",
      "data": {
        "values": [
          {"State": "NSW", "Value": 43.6},
